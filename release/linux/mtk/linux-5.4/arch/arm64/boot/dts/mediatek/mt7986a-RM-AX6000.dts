&pcie0 {
    pinctrl-names = "default";
    pinctrl-0 = <&pcie0_pins>;
    status = "okay";
};

&uart0 {
    status = "okay";
};

&spi0 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi_flash_pins>;
	status = "okay";
	cs-gpios = <0>, <0>;
	spi_nand: spi_nand@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-nand";
		reg = <1>;
		spi-max-frequency = <20000000>;
		spi-tx-buswidth = <4>;
		spi-rx-buswidth = <4>;
	};
};

&wbsys {
	status = "okay";
	mediatek,mtd-eeprom = <&factory 0x0000>;
/*
	pinctrl-names = "default", "dbdc";
	pinctrl-0 = <&wf_2g_5g_pins>;
	pinctrl-1 = <&wf_dbdc_pins>;
*/
};

&spi1 {
	pinctrl-names = "default";
    pinctrl-0 = <&spic_pins_g2>;
/*	status = "okay";*/
	miwifi-hm0807a-led@0 {
		compatible = "xiaomi,HM0807A";
		reg = <0x00>;
		spi-max-frequency = <3000000>;
		chip-num = <0x05>;
		led@0 {
			label = "led_red";
			default-state = "off";
			color = [ ff 00 00 ];
		};
		led@1 {
			label = "led_blue";
			default-state = "on";
			color = [ 00 00 ff ];
		};
		led@2 {
			label = "led_yellow";
			default-state = "off";
			color = [ ff 46 00 ];
		};
		led@3 {
			label = "led_white";
			default-state = "off";
			color = [ c0 ff c0 ];
		};
		led@4 {
			label = "led_green";
			default-state = "off";
			color = [ 00 ff 00 ];
		};
	};
};

&watchdog {
	status = "okay";
};

&i2c0 {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c_pins>;
	status = "okay";

	wm8960: wm8960@1a {
		compatible = "wlf,wm8960";
		reg = <0x1a>;
	};
};

&auxadc {
	status = "okay";
};
